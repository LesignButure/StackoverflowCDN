{
  "manifest_version": 2,
  "name": "StackOverflow CDN",
  "description": "Replace the files of cdn.sstatic.net with local one",
  "version": "1.0",
  "web_accessible_resources": ["stub.en.js", "all.css"],
  "permissions": ["webRequest", "webRequestBlocking",
                  "http://cdn.sstatic.net/*"],
  "background": {
    "scripts": ["background.js"]
  }
}